{
  "extends": "tslint:recommended",
  "rules": {
    "member-access": false,
    "max-classes-per-file": [
      false
    ],
    "switch-default": false,
    "max-line-length": [
      true,
      140
    ],
    "no-empty": false,
    "no-var-requires": false,
    "quotemark": false,
    "ordered-imports": false,
    "no-console": false,
    "arrow-parens": false,
    "variable-name": false,
    "trailing-comma": false,
    "radix": false,
    "object-literal-sort-keys": false
  }
}